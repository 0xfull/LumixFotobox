<Application
    x:Class="GMaster.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GMaster.Views"
    xmlns:converters="using:GMaster.Views.Converters"
    RequestedTheme="Light">
    <Application.Resources>
        <local:MainPageModel x:Key="MainModel" />
        <converters:VisibilityConverter x:Key="VisibilityConverter" />
        <converters:VisibilityNullConverter x:Key="VisibilityNullConverter" />
        <converters:NotVisibilityConverter x:Key="NotVisibilityConverter" />
        <converters:NotConverter x:Key="NotConverter" />
    </Application.Resources>
</Application>