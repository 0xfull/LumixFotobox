<Application
    x:Class="GMaster.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:GMaster.Views.Converters"
    xmlns:commands="using:GMaster.Views.Commands"
    xmlns:models="using:GMaster.Views.Models"
    RequestedTheme="Light">
    <Application.Resources>
        <models:MainPageModel x:Key="MainModel" />
        <converters:ZeroToVisibileConverter x:Key="ZeroToVisibileConverter" />
        <converters:NotZeroToVisibileConverter x:Key="NotZeroToVisibileConverter" />
        <converters:NotNullToVisibleConverter x:Key="NotNullToVisibleConverter" />
        <converters:NullToVisibleConverter x:Key="NullToVisibleConverter" />
        <converters:NullToFalseConverter x:Key="NullToFalseConverter" />
        <converters:NullToTrueConverter x:Key="NullToTrueConverter" />
        <converters:TrueToVisibileConverter x:Key="TrueToVisibileConverter" />
        <converters:FalseToVisibileConverter x:Key="FalseToVisibileConverter" />
        <converters:BoolNotConverter x:Key="BoolNotConverter" />
        <converters:SignalBarsToIconConverter x:Key="SignalBarsToIconConverter" />
        <commands:SwitchFullScreenCommand x:Key="SwitchFullScreenCommand" />
    </Application.Resources>
</Application>