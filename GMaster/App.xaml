<Application
    x:Class="GMaster.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GMaster.Views"
    xmlns:converters="using:GMaster.Views.Converters"
    xmlns:commands="using:GMaster.Views.Commands"
    RequestedTheme="Light">
    <Application.Resources>
        <local:MainPageModel x:Key="MainModel" />
        <converters:VisibilityConverter x:Key="VisibilityConverter" />
        <converters:VisibilityZeroConverter x:Key="VisibilityZeroConverter" />
        <converters:VisibilityNotZeroConverter x:Key="VisibilityNotZeroConverter" />
        <converters:VisibilityNotNullConverter x:Key="VisibilityNotNullConverter" />
        <converters:VisibilityNullConverter x:Key="VisibilityNullConverter" />
        <converters:NullFalseConverter x:Key="NullFalseConverter" />
        <converters:NullTrueConverter x:Key="NullTrueConverter" />
        <converters:NotVisibilityConverter x:Key="NotVisibilityConverter" />
        <converters:NotConverter x:Key="NotConverter" />
        <converters:SignalBarsConverter x:Key="SignalBarsConvertor" />
        <commands:SwitchFullScreenCommand x:Key="SwitchFullScreenCommand" />
    </Application.Resources>
</Application>